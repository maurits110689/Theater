@using Theater.Database
@model List<Product>
@{
    ViewData["Title"] = "Home Page";
}

<h1 class= "display-4" ></h1>
<div class="products">
    <div class="product">
        @foreach (var product in Model)
        {
            <h3>@product.Naam</h3>
            <p>@product.Prijs</p>
            <p>@product.Beschikbaarheid</p>
        }
    </div>
    }
</div>
