@using Theater.Database
@model List<Product>
@{
    ViewData["Title"] = "Home Page";
}

<div class="products">
    <h1 class="display-4">Welcome</h1>
    @foreach (var product in Model)
    {
    <div class="product">
        <h3>@product.Naam</h3>
        <p>@product.Prijs</p>
        <p>@product.Beschikbaarheid</p>
    </div>    
    }
   
   
</div>